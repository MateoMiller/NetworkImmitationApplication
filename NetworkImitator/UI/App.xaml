<Application x:Class="NetworkImitator.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ui="clr-namespace:NetworkImitator.UI"
             StartupUri="/UI/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ui:ComponentToBrushConverter x:Key="ComponentToBrushConverter" />
            <ui:EnumToVisibilityConverter x:Key="EnumToVisibilityConverter" />
            <ui:SelectionToBrushConverter x:Key="SelectionToBrushConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
